                                        case "Veeravasantham": var info="Varunapriya janya"; var notesText="S0 R2 G2 M1 P S1"; var notesText2 = "S1 N3 D3 P M1 G2 R2 S0 ";break;
                                        case "Vegavahini": var info="Chakravakam janya"; var notesText="S0 R1 G3 M1 D2 N2 D2 S1"; var notesText2 = "S1 N2 D2 P M1 G3 R1 S0 ";break;
                                        case "Velavali": var info="Gowrimanohari janya"; var notesText="S0 G2 M1 D2 N3 S1"; var notesText2 = "S1 N3 D2 P M1 G2 R2 S0 ";break;
                                        case "Velavali": var info="Gowrimanohari janya"; var notesText="S0 G2 R2 M1 P D2 S1"; var notesText2 = "S1 N3 D2 P M1 G2 R2 S0 ";break;
                                        case "Velavali": var info="Gowrimanohari janya"; var notesText="S0 R2 G2 M1 P D2 S1"; var notesText2 = "S1 N3 D2 P M1 G2 S0 ";break;
                                        case "Velavali": var info="Gowrimanohari janya"; var notesText="S0 R2 G2 S0 R2 M1 P D2 S1"; var notesText2 = "S1 N3 D2 P M1 G2 R2 S0 ";break;
                                        case "Velavali": var info="Gowrimanohari janya"; var notesText="S0 R2 M1 P D2 N3 S1"; var notesText2 = "S1 N3 D2 P M1 G2 R2 S0 ";break;
                                        case "Velavali": var info="Gowrimanohari janya"; var notesText="S0 R2 M1 P D2 S1"; var notesText2 = "S1 N3 D2 P M1 G2 R2 S0 ";break;
                                        case "Vijayabhooshavali": var info="Ghavambhodhi janya"; var notesText="S0 R1 G3 M2 P S1"; var notesText2 = "S1 N3 D3 P M2 G3 R1 S0 ";break;
                                        case "Vijayakoshalam": var info="Chitrambari janya"; var notesText="S0 R2 G3 M2 P S1"; var notesText2 = "S1 N3 P M2 G3 S0 ";break;
                                        case "Vijayamalavi": var info="Shamalangi janya"; var notesText="S0 R2 M2 P D1 S1"; var notesText2 = "S1 N1 D1 P M2 R2 S0 ";break;
                                        case "Vijayanagari": var info="Dharmavathi janya"; var notesText="S0 R2 G2 M2 P D2 S1"; var notesText2 = "S1 D2 P M2 G2 R2 S0 ";break;
                                        case "Vijayasaranga": var info="Hemavathi janya"; var notesText="S0 R2 G2 M2 P D2 S1"; var notesText2 = "S1 N2 D2 M2 G2 R2 S0 ";break;
                                        case "Vijayasaraswathi": var info="Simhendramadhyamam janya"; var notesText="S0 G2 M2 P D1 N3 S1"; var notesText2 = "S1 N3 P M2 G2 R2 S0 ";break;
                                        case "Vijayashrangi": var info="Hemavathi janya"; var notesText="S0 R2 G2 M2 P D2 S1"; var notesText2 = "S1 N2 D2 M2 G2 R2 S0 ";break;
                                        case "Vijayashree": var info="Suvarnangi janya"; var notesText="S0 R1 G2 M2 P N2 S1"; var notesText2 = "S1 N2 P M2 G2 R1 S0 ";break;
                                        case "Vijayavasantham": var info="Vishvambhari janya"; var notesText="S0 M2 P D3 N3 S1"; var notesText2 = "S1 N3 P M2 G3 S0 ";break;
                                        case "Vijayollasini": var info="Vakulabharanam janya"; var notesText="S0 R1 G3 M1 P M1 D1 N2 S1"; var notesText2 = "S1 N2 D1 P M1 G3 R1 S0 ";break;
                                        case "Vikhavathi": var info="Vagadeeshwari janya"; var notesText="S0 R3 G3 P D2 S1"; var notesText2 = "S1 D2 P G3 R3 S0 ";break;
                                        case "Vilasini": var info="Dheera-shankarabharanam janya"; var notesText="S0 R2 G3 M1 P N3 S1"; var notesText2 = "S1 N3 P M1 G3 R2 S0 ";break;
                                        case "Visharada": var info="Mayamalava-gowla janya"; var notesText="S0 R1 M1 P N3 S1"; var notesText2 = "S1 N3 P M1 R1 S0 ";break;
                                        case "Vishvambhari": var info="Melakarta 54, aka Gamanasrama"; var notesText="S0 R1 G3 M2 P D3 N3 S1"; var notesText2 = "S1 N3 D3 P M2 G3 R1 S0 ";break;
                                        case "Vishveshwarapriya": var info="Dharmavathi janya"; var notesText="S0 R2 M2 P N3 S1"; var notesText2 = "S1 N3 D2 P M2 R2 S0 ";break;
                                        case "Vittalapriya": var info="Vanaspati janya"; var notesText="S0 R1 M1 P D2 S1"; var notesText2 = "S1 D2 P M1 R1 S0 ";break;
                                        case "Vivahapriya": var info="Mechakalyani janya"; var notesText="S0 R2 M2 P D2 N3 S1"; var notesText2 = "S1 N3 D2 P M2 R2 S0 ";break;
                                        case "Vivardhani": var info="Harikambhoji Janya"; var notesText="S0 R2 M P S1"; var notesText2 = "S1 N2 D2 P M1 G3 R2 S0 ";break;
                                        case "Yadukula-kambhoji": var info="Harikambhoji Janya, combination of suddha saaveri and harikambhoji"; var notesText="S0 R2 M1 P D2 S1"; var notesText2 = "S1 N2 D2 P M1 G3 R2 S0 ";break;
                                        case "Yagapriya": var info="Melakarta 31"; var notesText="S0 R3 G3 M1 P D1 N1 S1"; var notesText2 = "S1 N1 D1 P M1 G3 R3 S0 ";break;
                                        case "Yagini": var info="Hemavathi janya"; var notesText="S0 R2 M2 P N2 S1"; var notesText2 = "S1 N2 P M2 R2 S0 ";break;
                                        case "Yaman": var info=""; var notesText="S0 R2 G3 M2 P D2 N3 S1"; var notesText2 = "S1 N3 D2 P M2 G3 M1 G3 R2 S0 ";break;
                                        case "Yaman": var info=""; var notesText="S0 R2 G3 M2 P D2 N3 S1"; var notesText2 = "S1 N3 D2 P M2 G3 R2 S0 ";break;
                                        case "Yamuna": var info=""; var notesText="S0 R2 G3 P M2 P D2 S1"; var notesText2 = "S1 D2 P M2 P G3 R2 S0 ";break;
                                        default: var info=""; var notesText = "notes"; var notesText2= "notes"; break;
				}
			}
			document.getElementById("info").value = info;
			document.getElementById("Arohanam").value = notesText;
			document.getElementById("Avarohanam").value = notesText2;
//			document.getElementById("AAnotes").value = notesText3;
		}
        function getSruthiFrequency() {
            var sruthiletter = document.getElementById("Sruthi").value;
            switch (sruthiletter) {
                case "A":
                    freq= 220;
                    break;
                case "A#":
                    freq= 233;
                case "B":
                    freq= 247;
                    break;
                case "C":
                    freq= 262;
                    break;
                case "C#":
                    freq= 277;
                    break;
                case "D":
                    freq= 294;
                    break;
                case "D#":
                    freq= 311;
                    break;
                case "E":
                    freq= 329;
                    break;
                case "F":
                    freq= 349;
                    break;
                case "F#":
                    freq= 370;
                    break;
                case "G":
                    freq= 392;
                    break;
                case "G#":
                    freq= 415;
                    break;
            }
			return freq;
        }      
		function durationchange() {
				document.getElementById("Durationvalue").innerHTML = document.getElementById("Duration").value;
		}
		function toggle() {
			var context = null;
			var usingWebAudio = true;	
			//this section is code for setting frequency
			
			if (typeof AudioContext !== 'undefined') {
				context = new AudioContext();
			} else if (typeof webkitAudioContext !== 'undefined') {
				if (navigator.appCodeName == "Firefox" || navigator.appCodeName == "Chrome") {
					context = new webkitAudioContext();
				} 
			} else {
				usingWebAudio = false;
			}
			var playing = false;
			var osc = null;
			//var freq = getSruthiFrequency();
			var noteslist = "";
			var duration = parseFloat(document.getElementById("Duration").value);
			
			getAAnotes();
			console.log("entering toggle");
			var freq = getSruthiFrequency();
			var arohanam = document.getElementById("Arohanam").value;
			var avarohanam = document.getElementById("Avarohanam").value;
			var prevraga = arohanam.concat(" zz ",avarohanam);
			if (playing && osc) {
				playing = false;
				osc.stop(0);
				button.value = "Play";
			} else {
				playing = true;
				button.value = "Playing";
				noteslist = prevraga;
				let array_notestext = noteslist.split(" ");
				for (var i = 0; i < array_notestext.length ; i++) {
                    
					//calculate frequency based on swara and sruthiletter
					switch (array_notestext[i]) {
						case "S0":
							freq2= freq*1; break;
						case "R1":
							freq2= freq*16/15; break;
						case "R1+":
							freq2= freq*31/15; break;
						case "R2":
							freq2= freq*9/8; break;
						case "R2+":
							freq2= freq*17/8; break;
						case "R3":
							freq2= freq*6/5; break;
						case "R3+":
							freq2= freq*11/5; break;
						case "G1":
							freq2= freq*9/8; break;
						case "G2":
							freq2= freq*6/5; break;
						case "G3":
							freq2= freq*5/4; break;
						case "G1+":
							freq2= freq*17/8; break;
						case "G2+":
							freq2= freq*11/5; break;
						case "G3+":
							freq2= freq*9/4; break;
						case "M1-":
							freq2= freq*1/3; break;
						case "M1":
							freq2= freq*4/3; break;
						case "M1+":
							freq2= freq*7/3; break;
						case "M2-":
							freq2= freq*5/12; break;
						case "M2":
							freq2= freq*17/12; break;
						case "M2+":
							freq2= freq*29/12; break;
						case "P-":
							freq2= freq*1/2; break;
						case "P":
							freq2= freq*3/2; break;
						case "P+":
							freq2= freq*5/2; break;
						case "D1":
							freq2= freq*8/5; break;
						case "D2":
							freq2= freq*5/3; break;
						case "D3":
							freq2= freq*9/5; break;
						case "D1-":
							freq2= freq*3/5; break;
						case "D2-":
							freq2= freq*2/3; break;
						case "D3-":
							freq2= freq*4/5; break;
						case "N1":
							freq2= freq*5/3; break;
						case "N1-":
							freq2= freq*2/3; break;
						case "N2":
							freq2= freq*9/5; break;
						case "N2-":
							freq2= freq*4/5; break;
						case "N3":
							freq2= freq*15/8; break;
						case "N3-":
							freq2= freq*7/8; break;
						case "S1":
							freq2= freq*2; break;
						case "zz":
							freq2 = 0;break;
						default:
							break;
					}
                   
                    
					console.log("here");
					osc = context.createOscillator();
					osc.connect(context.destination);
					osc.frequency.value = freq2;
					osc.start(duration*i);
					osc.stop(duration*(i+1));
				}	
				button.value = "Play";
				playing = false;
				osc.stop(0);
			}
        }
    //END OF CODE TO GENERATE FREQUENCY TONE
		
    </script>
	</input>



</body>
</html>
