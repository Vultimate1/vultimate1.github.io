					case "Vishveshwarapriya":var notesText="S0 R2 M2 P N3 S1"; var notesText2 = "S1 N3 D2 P M2 R2 S0 ";break;
					case "Vittalapriya":var notesText="S0 R1 M1 P D2 S1"; var notesText2 = "S1 D2 P M1 R1 S0 ";break;
					case "Vivahapriya":var notesText="S0 R2 M2 P D2 N3 S1"; var notesText2 = "S1 N3 D2 P M2 R2 S0 ";break;
					case "Vivardhani":var notesText="S0 R2 M P S1"; var notesText2 = "S1 N2 D2 P M1 G3 R2 S0 ";break;
					case "Yadukula-khambohi":var notesText="S0 R2 M1 P D2 S1"; var notesText2 = "S1 N2 D2 P M1 G3 R2 S0 ";break;
					case "Yagapriya":var notesText="S0 R3 G3 M1 P D1 N1 S1"; var notesText2 = "S1 N1 D1 P M1 G3 R3 S0 ";break;
					case "Yagini":var notesText="S0 R2 M2 P N2 S1"; var notesText2 = "S1 N2 P M2 R2 S0 ";break;
					case "Yaman":var notesText="S0 R2 G3 M2 P D2 N3 S1"; var notesText2 = "S1 N3 D2 P M2 G3 M1 G3 R2 S0 ";break;
					case "Yaman":var notesText="S0 R2 G3 M2 P D2 N3 S1"; var notesText2 = "S1 N3 D2 P M2 G3 R2 S0 ";break;
					case "Yamuna":var notesText="S0 R2 G3 P M2 P D2 S1"; var notesText2 = "S1 D2 P M2 P G3 R2 S0 ";break;
					default:var notesText = "notes"; var notesText2= "notes"; break;
				}
			}
//			var notesText3 = notesText.concat(" ",notesText2);
			document.getElementById("arohanam").value = notesText;
			document.getElementById("avarohanam").value = notesText2;
//			document.getElementById("AAnotes").value = notesText3;
		}
        function getSruthiFrequency() {
            var sruthiletter = document.getElementById("sruthi").value;
            switch (sruthiletter) {
                case "A":
                    freq= 220;
                    break;
                case "A#":
                    freq= 233;
                case "B":
                    freq= 247;
                    break;
                case "C":
                    freq= 262;
                    break;
                case "C#":
                    freq= 277;
                    break;
                case "D":
                    freq= 294;
                    break;
                case "D#":
                    freq= 311;
                    break;
                case "E":
                    freq= 329;
                    break;
                case "F":
                    freq= 349;
                    break;
                case "F#":
                    freq= 370;
                    break;
                case "G":
                    freq= 392;
                    break;
                case "G#":
                    freq= 415;
                    break;
            }
			return freq;
        }      
		function toggle() {
			var context = null;
			var usingWebAudio = true;	
			//this section is code for setting frequency
			
			if (typeof AudioContext !== 'undefined') {
				context = new AudioContext();
			} else if (typeof webkitAudioContext !== 'undefined') {
				if (navigator.appCodeName == "Firefox" || navigator.appCodeName == "Chrome") {
					context = new webkitAudioContext();
				} 
			} else {
				usingWebAudio = false;
			}
			var playing = false;
			var osc = null;
			//var freq = getSruthiFrequency();
			var noteslist = "";
			var duration = 0.6;//parseFloat(document.getElementById("duration").value);
			getAAnotes();
			console.log("entering toggle");
			var freq = getSruthiFrequency();
			var arohanam = document.getElementById("arohanam").value;
			var avarohanam = document.getElementById("avarohanam").value;
			var prevraga = arohanam.concat(" zz ",avarohanam);
			if (playing && osc) {
				playing = false;
				osc.stop(0);
				button.value = "Play";
			} else {
				playing = true;
				button.value = "Playing";
				noteslist = prevraga;
				let array_notestext = noteslist.split(" ");
				for (var i = 0; i < array_notestext.length ; i++) {
                    
					//calculate frequency based on swara and sruthiletter
					switch (array_notestext[i]) {
						case "S0":
							freq2= freq*1; break;
						case "R1":
							freq2= freq*16/15; break;
						case "R1+":
							freq2= freq*31/15; break;
						case "R2":
							freq2= freq*9/8; break;
						case "R2+":
							freq2= freq*17/8; break;
						case "R3":
							freq2= freq*6/5; break;
						case "R3+":
							freq2= freq*11/5; break;
						case "G1":
							freq2= freq*9/8; break;
						case "G2":
							freq2= freq*6/5; break;
						case "G3":
							freq2= freq*5/4; break;
						case "G1+":
							freq2= freq*17/8; break;
						case "G2+":
							freq2= freq*11/5; break;
						case "G3+":
							freq2= freq*9/4; break;
						case "M1-":
							freq2= freq*1/3; break;
						case "M1":
							freq2= freq*4/3; break;
						case "M1+":
							freq2= freq*7/3; break;
						case "M2-":
							freq2= freq*5/12; break;
						case "M2":
							freq2= freq*17/12; break;
						case "M2+":
							freq2= freq*29/12; break;
						case "P-":
							freq2= freq*1/2; break;
						case "P":
							freq2= freq*3/2; break;
						case "P+":
							freq2= freq*5/2; break;
						case "D1":
							freq2= freq*8/5; break;
						case "D2":
							freq2= freq*5/3; break;
						case "D3":
							freq2= freq*9/5; break;
						case "N1":
							freq2= freq*5/3; break;
						case "N1-":
							freq2= freq*2/3; break;
						case "N2":
							freq2= freq*9/5; break;
						case "N2-":
							freq2= freq*4/5; break;
						case "N3":
							freq2= freq*15/8; break;
						case "N3-":
							freq2= freq*7/8; break;
						case "S1":
							freq2= freq*2; break;
						case "zz":
							freq2 = 0;break;
						default:
							break;
					}
                   
                    
					console.log("here");
					osc = context.createOscillator();
					osc.connect(context.destination);
					osc.frequency.value = freq2;
					osc.start(duration*i);
					osc.stop(duration*(i+1));
				}	
				button.value = "Play";
				playing = false;
				osc.stop(0);
			}
        }
    //END OF CODE TO GENERATE FREQUENCY TONE
		
    </script>
	</input>



</body>
</html>
