                                        case "Arabi": var info="Dheera-shankarabharanam janya"; var notesText="S0 R2 M1 P D2 S1"; var notesText2 = "S1 N3 D2 P M1 G3 R2 S0 ";break;
                                        case "Ardhradesi": var info="Mayamalava-gowla janya"; var notesText="S0 R1 G3 M1 P D1 N3 S1"; var notesText2 = "S1 D1 P M1 G3 R1 S0 ";break;
                                        case "Arunangi": var info="Shubhapanthuvarali janya"; var notesText="S0 R1 M2 P N3 D1 S1"; var notesText2 = "S1 N3 D1 M2 R1 G2 R1 S0 ";break;
                                        case "Aryamati": var info="Kharaharapriya janya"; var notesText="S0 R2 G2 P D2 S1"; var notesText2 = "S1 N2 D2 P D2 M1 G2 R2 S0 ";break;
                                        case "Asaveri": var info="Hanumatodi janya"; var notesText="S0 R1 M1 P D1 S1"; var notesText2 = "S1 N2 S1 P D1 M1 P R1 G2 R1 S0 ";break;
                                        case "Atana": var info=""; var notesText="S0 R2 M1 P N3 S1"; var notesText2 = "S1 N3 D2 P M1 P G3 R2 S0 ";break;
                                        case "Aymmukhan": var info="Keeravani janya"; var notesText="S0 G2 M1 P D1 N3 S1"; var notesText2 = "S1 N3 D1 P M1 G2 S0 ";break;
