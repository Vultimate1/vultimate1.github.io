                                        case "Bangala": var info="Dheera-shankarabharanam janya"; var notesText="S0 R2 G3 M1 P M1 R2 P S1"; var notesText2 = "S1 N3 P M1 R2 G3 R2 S0 ";break;
                                        case "Basant-bahar": var info="Kharaharapriya janya"; var notesText="S0 M2 P G3 M2 N3 D1 N3 S1"; var notesText2 = "S1 R2+ S1 N2 D2 P M1 G2 M1 G2 R2 S0 ";break;
                                        case "Begada": var info="Dheera-shankarabharanam janya"; var notesText="S0 G3 R2 G3 M1 P D2 N2 D2 P S1"; var notesText2 = "S1 N3 D2 P M1 G3 R2 S0 ";break;
                                        case "Behag": var info="Dheera-shankarabharanam janya"; var notesText="S0 G3 M1 P N3 D2 N3 S1"; var notesText2 = "S1 N3 D2 P M1 G3 R2 S0 ";break;
                                        case "Behag-deshikam": var info=""; var notesText="S0 R2 G3 M1 M2 P D2 N3 S1"; var notesText2 = "S1 N3 D2 P M2 M1 G3 R2 S0 ";break;
