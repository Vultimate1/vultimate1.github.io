    <script type="text/JavaScript"> 
		var button = document.getElementById("playragam");
        button.text = "Play";
		function populate() {
			getAAnotes();
		}
		function getAAnotes() {
            var ragaselect = document.querySelector('#raganame').value;
		//	var ragaselect = document.getElementById("raganame").value;
			var pattern=new RegExp("[ABCDEFGHIJKLabcdefghijkl]");
			if (pattern.test(ragaselect[0])) {
				//console.log("A to l");
				switch (ragaselect) {
										case "Abheri": var info="Kharaharapriya janya, audava-sampoorna"; var notesText="S0 G2 M1 P N2 S1"; var notesText2 = "S1 N2 D2 P M1 G2 R2 S0 ";break;
                                        case "Abhiramam": var info="Dhavalambari janya"; var notesText="S0 R1 M2 P D1 N1 S1"; var notesText2 = "S1 N1 D1 P M2 G2 R1 S0 ";break;
                                        case "Abhiru": var info="Suvarnangi janya"; var notesText="S0 R1 G2 R1 M2 P N3 S1"; var notesText2 = "S1 D2 P M2 G2 R1 G2 S0 ";break;
                                        case "Abhogi": var info="Kharaharapriya janya"; var notesText="S0 R2 G2 M1 D2 S1"; var notesText2 = "S1 D2 M1 G2 R2 S0 ";break;
                                        case "Adhi-panchama": var info="Panthuvarali janya"; var notesText="S0 R1 P D1 N3 S1"; var notesText2 = "S1 N3 D1 N3 P M2 G3 R1 S0 ";break;
                                        case "Adi-kapi": var info="Kharaharapriya janya"; var notesText="S0 R2 M1 G2 M1 P D2 N2 S1"; var notesText2 = "S1 N2 D2 P M1 G2 R2 S0 ";break;
                                        case "Agnikopa": var info="Kharaharapriya janya"; var notesText="S0 G2 M1 P N2 S1"; var notesText2 = "S1 N2 P M1 G2 R2 S0 ";break;
                                        case "Ahir-bhairavi": var info="Chakravakam janya"; var notesText="S0 R1 G3 M1 P N2 D2 M1 P D2 S1"; var notesText2 = "S1 N2 D2 P M1 P G3 R1 S0 ";break;
                                        case "Ahiri": var info="Hanumatodi janya"; var notesText="S0 R1 S0 G3 M1 P D1 N2 S1"; var notesText2 = "S1 N2 D1 P M1 G3 R1 S0 ";break;
                                        case "Alankarapriya": var info="Natakapriya janya"; var notesText="S0 R1 G2 M1 D2 N2 S1"; var notesText2 = "S1 N2 D2 M1 G2 R1 S0 ";break;
                                        case "Alankari": var info="Gamanasrama janya"; var notesText="S0 G3 M2 D2 N3 D2 S1"; var notesText2 = "S1 N3 D2 M2 G3 S0 ";break;
                                        case "Amarasenapriya": var info="Nitimati janya"; var notesText="S0 R2 M2 P N2 S1"; var notesText2 = "S1 N3 P M2 G2 R2 S0 ";break;
                                        case "Ambhojini": var info="Harikambhoji janya"; var notesText="S0 R2 G3 M1 D2 S1"; var notesText2 = "S1 D2 M1 G3 R2 S0 ";break;
                                        case "Amrita-dhanyasi": var info="Hanumatodi janya"; var notesText="S0 R1 G2 M1 P N2 S1"; var notesText2 = "S1 N2 P M1 G2 R1 S0 ";break;
                                        case "Amritavarshini": var info="Chitrambari janya"; var notesText="S0 G3 M2 P N3 S1"; var notesText2 = "S1 N3 P M2 G3 S0 ";break;
                                        case "Amritha-behag": var info="Mechakalyani janya"; var notesText="S0 M2 G3 P N3 S1 D N3 D2 M2 G3 S0 ";break;
                                        case "Amrithavahini": var info="Natabhairavi janya"; var notesText="S0 R2 M1 P D1 N2 S1"; var notesText2 = "S1 N2 D1 M1 G2 R2 S0 ";break;
                                        case "Amudagandhari": var info="Ragavardhini janya"; var notesText="S0 G3 M1 P D1 N2 S1"; var notesText2 = "S1 N2 D1 P M1 G3 R3 S0 ";break;
                                        case "Amudhasurabhi": var info="Vakulabharanam janya"; var notesText="S0 M1 G3 M1 P D1 S1"; var notesText2 = "S1 N2 D1 P M1 R1 S0 ";break;
                                        case "Anandabhairavi": var info="Natabhairavi janya, evening raga"; var notesText="S0 G2 R2 G2 M1 P D2 P S1"; var notesText2 = "S1 N2 D2 P M1 G2 R2 S0 ";break;
                                        case "Anandavalli": var info="Simhendramadhyamam janya"; var notesText="S0 G2 M2 P N3 S1"; var notesText2 = "S1 N3 P M2 G2 S0 ";break;
                                        case "Anandharoopa": var info="Dheera-shankarabharanam janya"; var notesText="S0 R2 G3 P D2 N3 S1"; var notesText2 = "S1 N3 D2 P G3 R2 S0 ";break;
                                        case "Andhali": var info="Harikambhoji janya"; var notesText="S0 R2 M1 P N2 S1"; var notesText2 = "S1 N2 P M1 R2 G3 M1 R2 S0 ";break;
                                        case "Andolika": var info="Kharaharapriya janya"; var notesText="S0 R2 M1 P N2 S1"; var notesText2 = "S1 N2 D2 M1 R2 S0 ";break;
