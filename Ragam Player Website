    <script type="text/JavaScript"> 
		var button = document.getElementById("playragam");
        button.text = "Play";
		function getAAnotes() {
			var ragaselect = document.getElementById("raga").value;
			var pattern=new RegExp("[ABCDEFGHIJKLabcdefghijkl]");
			if (pattern.test(ragaselect[0])) {
				//console.log("a to l");
				switch (ragaselect) {
					case "Abheri":var notesText="S0 G2 M1 P N2 S1"; var notesText2 = "S1 N2 D2 P M1 G2 R2 S0 ";break;
					case "Abhiramam":var notesText="S0 R1 M2 P D1 N1 S1"; var notesText2 = "S1 N1 D1 P M2 G2 R1 S0 ";break;
					case "Abhiru":var notesText="S0 R1 G2 R1 M2 P N3 S1"; var notesText2 = "S1 D2 P M2 G2 R1 G2 S0 ";break;
					case "Abhogi":var notesText="S0 R2 G2 M1 D2 S1"; var notesText2 = "S1 D2 M1 G2 R2 S0 ";break;
					case "adhi-panchama":var notesText="S0 R1 P D1 N3 S1"; var notesText2 = "S1 N3 D1 N3 P M2 G3 R1 S0 ";break;
					case "adi-kapi":var notesText="S0 R2 M1 G2 M1 P D2 N2 S1"; var notesText2 = "S1 N2 D2 P M1 G2 R2 S0 ";break;
					case "Agnikopa":var notesText="S0 G2 M1 P N2 S1"; var notesText2 = "S1 N2 P M1 G2 R2 S0 ";break;
					case "ahir-bhairavi":var notesText="S0 R1 G3 M1 P N2 D2 M1 P D2 S1"; var notesText2 = "S1 N2 D2 P M1 P G3 R1 S0 ";break;
					case "ahiri":var notesText="S0 R1 S0 G3 M1 P D1 N2 S1"; var notesText2 = "S1 N2 D1 P M1 G3 R1 S0 ";break;
					case "Alankarapriya":var notesText="S0 R1 G2 M1 D2 N2 S1"; var notesText2 = "S1 N2 D2 M1 G2 R1 S0 ";break;
					case "Alankari":var notesText="S0 G3 M2 D2 N3 D2 S1"; var notesText2 = "S1 N3 D2 M2 G3 S0 ";break;
					case "Amarasenapriya":var notesText="S0 R2 M2 P N2 S1"; var notesText2 = "S1 N3 P M2 G2 R2 S0 ";break;
					case "Ambhojini":var notesText="S0 R2 G3 M1 D2 S1"; var notesText2 = "S1 D2 M1 G3 R2 S0 ";break;
					case "amrita-dhanyasi":var notesText="S0 R1 G2 M1 P N2 S1"; var notesText2 = "S1 N2 P M1 G2 R1 S0 ";break;
					case "Amritavarshini":var notesText="S0 G3 M2 P N3 S1"; var notesText2 = "S1 N3 P M2 G3 S0 ";break;
					case "amritha-behag":var notesText="S0 M2 G3 P N3 S1 D N3 D2 M2 G3 S0 ";break;
					case "Amrithavahini":var notesText="S0 R2 M1 P D1 N2 S1"; var notesText2 = "S1 N2 D1 M1 G2 R2 S0 ";break;
					case "Amudagandhari":var notesText="S0 G3 M1 P D1 N2 S1"; var notesText2 = "S1 N2 D1 P M1 G3 R3 S0 ";break;
					case "Amudhasurabhi":var notesText="S0 M1 G3 M1 P D1 S1"; var notesText2 = "S1 N2 D1 P M1 R1 S0 ";break;
